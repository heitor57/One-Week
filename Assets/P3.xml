	<dialogos>
		<dialogo texto="missao">
			<dialogo texto="Capitão, preciso lhe contar algo...">
				<dialogo texto="Calma, fique calmo.">
					<dialogo texto="Nosso rei, ele corre perigo, um golpe está sendo planejado.">
						<dialogo texto="Diga o que sabe...">
							<dialogo texto="O golpe vem de dentro do castelo, não posso falar mais...não confie em ninguém.">
								<dialogo texto="Do castelo? Como posso ajudar.">
									<dialogo texto="Procure pelo bibliotecário de Boreas,">
										<dialogo texto="O senhor Novaes, ele pode lhe contar tudo.">
											<dialogo texto="Novaes? Era ver ele que eu iria mesmo."> </dialogo>
										</dialogo>
										<dialogo texto="Tem certeza? Aquele cara anda muito estranho nos ultimos tempos, ">
											<dialogo texto="vou pensar sobre isso."></dialogo>
										</dialogo>
								</dialogo>
							</dialogo>
						</dialogo>
					</dialogo>
				</dialogo>
			</dialogo>
		</dialogo>
	</dialogo>
